                    <condition colname="CODE" value="HELMET" canMiss="true" searchMetaId="player_stock_STOCKID"/>
                    <condition colname="CODE" value="WRISTER" canMiss="true" searchMetaId="player_stock_STOCKID"/>
                    <condition colname="CODE" value="SHOES" canMiss="true" searchMetaId="player_stock_STOCKID"/>
                    <condition colname="CODE" value="CLOAK" canMiss="true" searchMetaId="player_stock_STOCKID"/>
