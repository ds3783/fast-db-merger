        <task type="sql" name="pre2" side="src"><![CDATA[UPDATE `users` SET `NAME`=TRIM(`NAMEsrc`)]]></task>
        <task type="sql" name="pre3" side="src"><![CDATA[ALTER TABLE `users` DROP INDEX USERNAME]]></task>
